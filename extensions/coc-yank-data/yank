d34899df160ea7b6d922d66574d1ecad|file:///Users/om/Google%20Drive/OmMahesh/Programming/Comp%20Prog/Templates/std%20io/main.cpp|451|1|V|cpp
	
	// search through a sorted vector with binary search
	tcT int se(V<T> a, T b) {
	  int m;
	  for (int l = 0, r = sz(a); r - l > 1;) {
	    m = (l + r) / 2;
	    if (b == a[m])
	      rtn m;
	    else if (b > a[m])
	      l = m;
	    else
	      r = m;
	  }
	  if (b > a[m])
	    rtn(m + 2);
	  else
	    rtn(m);
	}
79be7bc374a611798fcc23bad5dd1087|file:///Users/om/Google%20Drive/OmMahesh/Programming/Comp%20Prog/Templates/std%20io/main.cpp|469|1|V|cpp
	
	// get the lower bound of se
	tcT int lse(V<T> a, T b) {
	  int c = se(a, b);
	  while (a[c] == b && c >= 0) c--;
	  rtn (c + 1);
	}
