	  vi ans;
	  if (a == 0 && (a + b) % 2) {
	    ans.pb((a + b) / 2);
	    ans.pb((a + b + 1) / 2);
	    goto a;
	  }
	  if ((a + b) % 2 == 0) {
	    int x = (a + b) / 2;
	    int y = x - a;
	    FOR(i, 0, a + 1) {
	      ans.pb(y);
	      y += 2;
	    }
	  } else {
	    FOR(i, 1, b + 1) ans.pb(i);
	    if (b - a == 1) ans.pb(0);
	  }
