	tcT T amaxi(T a) {
	  v mi{0};
	  FOR(i, sz(a)) {
	    if (a[i] > a[mi[0]]) mi.clear(), mi.pb(i);
	    else if (a[i] == a[mi[0]]) mi.pb(i);
	  }
	  rtn mi;
	}
