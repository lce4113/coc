5f31bc676b83bc204a07c159dc318729|file:///Users/om/Google%20Drive/OmMahesh/Programming/Comp%20Prog/CodeForces/Round%20%23710/Q4/d.cpp|773|1|V|cpp
	
	  pq b;
	  each(a, x) b.push(x.s);
	
	  while (true) {
	    out(b);
	    int c = b.top();
	    b.pop();
	    if (!sz(b)) {
	      out(c);
	      brk;
	    }
	    int d = b.top();
	    b.pop();
	    if (d == 0) {
	      out(c);
	      brk;
	    }
	    b.push(c - d);
	    b.push(0);
	  }
	}
	
