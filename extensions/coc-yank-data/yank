	  vp b;
	  int K = 0, prev = a[0];
	  FOR(i, 0, n - 1) {
	    if (a[i] > a[i + 1]) {
	      b.pb({prev, a[i]});
	      prev = a[i + 1];
	      K++;
	    }
	  }
	
	  out(K);
	
	  if (K >= k) {
	    goto a;
	  }
