	// exponent function
	tcT T exp(T a, T b, bool m = true) {
	  T res = 1;
	  FOR(b) {
	    res *= a;
	    if (m) res %= m;
	  }
	  rtn res;
	}
49373ad5b4e923df1d3bca1e8458b6bf|file:///Users/om/Google%20Drive/OmMahesh/Programming/Comp%20Prog/Templates/std%20io/main.cpp|391|1|V|cpp
	
	// prime factorize an integer
	tcT M<T, T> pfac(T a) {
	  M<T, T> ans;
	  T n = a;
	  for (int i = 2; i * i <= a; i++) {
	    while (n % i == 0) {
	      n /= i;
	      ans[i]++;
	    }
	  }
	  if (n > 1) ans[n]++;
	  rtn ans;
	}
