	  int n, k;
	  in(n, k);
	
	  V<int> a(n);
	  in(a);
	
	  auto b = addind(a);
	  sor(b);
	
	  int x = 0;
	  FOR(i, 0, n - 1) {
	    while (b[i].s + 1 == b[i + 1].s && i < n - 1) i++, x++;
	  }
	  x = n - x;
	
	  out(a, x);
	
	  if (x > k) {
	    goto a;
	  }
	
	  out("YES");
	  rtn;
	a:
	  out("NO");
