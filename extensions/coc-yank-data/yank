ed3abb19b6991c1d6dc5229b0912b15b|file:///Users/om/Google%20Drive/OmMahesh/Programming/Comp%20Prog/Templates/std%20io/main.cpp|651|1|V|cpp
	
	tcT vi amini(T a) {
	  vi mi{0};
	  FOR(i, 1, sz(a)) {
	    if (a[i] < a[mi[0]])
	      mi.clear(), mi.pb(i);
	    else if (a[i] == a[mi[0]])
	      mi.pb(i);
	  }
	  rtn mi;
	}
