9e8b9cda63ae7755da9100bff822192b|file:///Users/om/Google%20Drive/OmMahesh/Programming/Comp%20Prog/Templates/std%20io/main.cpp|37|1|V|cpp
	
	// Mod Calculations {{{
	
	const int MOD = 1e9 + 7;
	tcT int mod(T n) { rtn(n % MOD); }
	
	bool is_mod();
	tcT T add(T a, T b) { rtn is_mod() ? mod(a + b) : a + b; };
	tcT T mult(T a, T b) { rtn is_mod() ? mod(mod(a) * mod(b)) : a *b; };
	
	// }}}
